{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "prefer-const": "warn",
    "no-var": "warn",
    "no-console": "off",
    "eqeqeq": "warn",
    "react/no-unescaped-entities": "off",
    "@next/next/no-html-link-for-pages": "off",
    "@next/next/no-img-element": "off",
    "react-hooks/exhaustive-deps": "off"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
